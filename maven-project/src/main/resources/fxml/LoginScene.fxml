<AnchorPane fx:id="loginPane" prefHeight="383.0" prefWidth="314.0" stylesheets="@../css/loginPaneDARK.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="loginPackage.LoginController">
    <children>
      <ImageView fx:id="avaterIMG" fitHeight="113.0" fitWidth="112.0" layoutX="102.0" layoutY="61.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../icons/user.png" />
         </image>
      </ImageView>
      <ImageView fx:id="imageVCancelBT" fitHeight="32.0" fitWidth="32.0" layoutX="259.0" layoutY="38.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../icons/cancelmusic.png" />
         </image>
      </ImageView>
      <JFXTextField fx:id="userNameField" layoutX="54.0" layoutY="221.0" maxWidth="206.0" minWidth="206.0" prefHeight="36.0" prefWidth="206.0" promptText="Username">
         <font>
            <Font size="14.0" />
         </font>
      </JFXTextField>
      <JFXPasswordField fx:id="passwordField" layoutX="55.0" layoutY="273.0" maxWidth="206.0" minWidth="206.0" prefHeight="36.0" prefWidth="206.0" promptText="Password">
         <font>
            <Font size="14.0" />
         </font>
      </JFXPasswordField>
      <Button fx:id="closeButton" layoutX="259.0" layoutY="29.0" mnemonicParsing="false" onAction="#closeWindow" prefHeight="32.0" prefWidth="32.0" style="-fx-background-color: transparent;" />
      <JFXToggleButton fx:id="toggleButton" layoutX="14.0" layoutY="10.0" onAction="#changeFont" />
      <JFXButton fx:id="loginBT" defaultButton="true" layoutX="247.0" layoutY="330.0" onAction="#loginAction" text="Login" />
      <Text fx:id="falseInputField" layoutX="126.0" layoutY="351.0" strokeType="OUTSIDE" strokeWidth="0.0" text="false input" visible="false" />
      <JFXButton fx:id="registerBT" defaultButton="true" layoutX="24.0" layoutY="330.0" onAction="#loginAction" text="Register" />
        <JFXTextField fx:id="nameField" disable="true" layoutX="55.0" layoutY="177.0" maxWidth="206.0" minWidth="206.0" opacity="0.0" prefHeight="36.0" prefWidth="206.0" promptText="Name">
            <font>
                <Font size="14.0" />
            </font>
        </JFXTextField>
    </children>
