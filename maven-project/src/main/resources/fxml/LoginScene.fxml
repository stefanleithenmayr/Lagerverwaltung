<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXPasswordField?>
<?import com.jfoenix.controls.JFXTextField?>
<?import com.jfoenix.controls.JFXToggleButton?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="loginPane" prefHeight="383.0" prefWidth="314.0" style="-fx-background-radius: 10;" stylesheets="@../css/loginPaneDARK.css" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="loginPackage.LoginController">
    <children>
      <JFXButton defaultButton="true" layoutX="252.0" layoutY="323.0" onAction="#loginAction" style="-fx-background-color: #3D4956;" text="Login" textFill="#bcaaaa" />
      <ImageView fx:id="avaterIMG" fitHeight="113.0" fitWidth="112.0" layoutX="102.0" layoutY="61.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../icons/user.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="32.0" fitWidth="32.0" layoutX="259.0" layoutY="38.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../icons/cross-symbol.png" />
         </image>
      </ImageView>
      <JFXTextField fx:id="userNameField" focusColor="WHITE" layoutX="54.0" layoutY="221.0" maxWidth="206.0" minWidth="206.0" prefHeight="36.0" prefWidth="206.0" promptText="Username" style="-fx-text-fill: white; -fx-prompt-text-fill: white;" unFocusColor="WHITE">
         <font>
            <Font size="14.0" />
         </font>
      </JFXTextField>
      <JFXPasswordField fx:id="passwordField" focusColor="WHITE" layoutX="55.0" layoutY="273.0" maxWidth="206.0" minWidth="206.0" prefHeight="36.0" prefWidth="206.0" promptText="Password" style="-fx-text-fill: white; -fx-prompt-text-fill: white;" unFocusColor="WHITE">
         <font>
            <Font size="14.0" />
         </font>
      </JFXPasswordField>
      <Button fx:id="closeButton" layoutX="259.0" layoutY="29.0" mnemonicParsing="false" onAction="#closeWindow" prefHeight="32.0" prefWidth="32.0" style="-fx-background-color: transparent;" />
      <JFXTextField fx:id="falseInputField" alignment="TOP_LEFT" editable="false" focusColor="#3d4956" layoutX="115.0" layoutY="318.0" maxWidth="86.0" minWidth="71.0" prefHeight="36.0" prefWidth="86.0" style="-fx-text-fill: red;" text="false input" unFocusColor="#3d4956" visible="false">
         <font>
            <Font size="14.0" />
         </font>
      </JFXTextField>
      <JFXToggleButton fx:id="toggleButton" layoutX="14.0" layoutY="10.0" onAction="#changeFont" />
    </children>
</AnchorPane>
